# ~/.ssh/config

Host gist.github.com
    Host gist.github.com
    User git
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/jehan_personal_id_ecdsa

Host github.com
    Host github.com
    User git
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/jehan_personal_id_ecdsa


host *.local
	User jehan
	IdentityFile ~/.ssh/jehan_personal_id_ecdsa


host *.z8s.io *.zulily.com
	user q1440733
	IdentityFile ~/.ssh/id_ecdsa
