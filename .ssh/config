# ~/.ssh/config

Host gist.github.com
    Host gist.github.com
    User git
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/jehan_personal_id_ecdsa

Host github.com
    Host github.com
    User git
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/jehan_personal_id_ecdsa


host *.local
	User jehan
	IdentityFile ~/.ssh/jehan_personal_id_ecdsa



Host *
	IdentityAgent "~/Library/Group Containers/2BUA8C4S2C.com.1password/t/agent.sock"